        <Field Name="Image">
          <DataField>Image</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>BXBook</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select top 10 Title, avg(BookRating) as Rating, BXBook.Image
from BXBookRating join BXBook on BXBook.ISBN = BXBookRating.ISBN
group by BXBook.ISBN, BXBook.Title, BXBook.Image
having count(BXBookRating.ISBN) &gt; 20
order by Rating desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
