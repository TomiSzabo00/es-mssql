        </Field>
      </Fields>
      <Query>
        <DataSourceName>BYBook</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select Country, count(*) as Count, avg(Age) as AvgAge
from BXUser
where Country is not NULL and Country&lt;&gt;''
group by Country
having count(*) &gt;= 50
order by Count desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
